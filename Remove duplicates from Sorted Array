

arr = [1, 1, 2, 2, 3, 3, 4, 4]

i=0
j=0
size = len(arr)-1
    
while j <= size:
    if arr[i] != arr[j]:
        i += 1
        arr[i] = arr[j]
    j+=1
        
print(arr[:i])





